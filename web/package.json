{
  "name": "web",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "build:login": "vite build --config vite.login.config.ts",
    "type-check": "vue-tsc --build",
    "lint": "run-s lint:oxlint lint:eslint",
    "lint:oxlint": "oxlint .",
    "lint:eslint": "pnpm --dir .. exec eslint web --max-warnings 0",
    "lint:style": "stylelint \"src/**/*.{css,vue,html}\" --allow-empty-input",
    "lint:fix": "run-s lint:oxlint:fix lint:eslint:fix lint:style:fix",
    "lint:oxlint:fix": "oxlint . --fix",
    "lint:eslint:fix": "pnpm --dir .. exec eslint web --fix --max-warnings 0",
    "lint:style:fix": "stylelint \"src/**/*.{css,vue,html}\" --fix --allow-empty-input",
    "format": "prettier --write --experimental-cli src/"
  },
  "dependencies": {
    "lucide-vue-next": "^0.575.0",
    "pinia": "^3.0.4",
    "vue": "^3.5.28",
    "vue-router": "^5.0.3"
  },
  "devDependencies": {
    "@tsconfig/node24": "^24.0.4",
    "@types/node": "^24.10.13",
    "@vitejs/plugin-vue": "^6.0.4",
    "@vue/tsconfig": "^0.8.1",
    "jiti": "^2.6.1",
    "npm-run-all2": "^8.0.4",
    "oxlint": "~1.47.0",
    "postcss-html": "^1.8.0",
    "prettier": "3.8.1",
    "stylelint": "^17.3.0",
    "stylelint-config-standard": "^40.0.0",
    "typescript": "~5.9.3",
    "vite": "^7.3.1",
    "vite-plugin-vue-devtools": "^8.0.6",
    "vue-tsc": "^3.2.4"
  },
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  }
}
